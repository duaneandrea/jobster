<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="shortcut icon" href="https://i.ibb.co/NZDtJdJ/jobster-logo.png" type="image/png">
        <link href="{{ app.request.getSchemeAndHttpHost() }}/assets/css/bootstrap.css" rel="stylesheet">
        <link href="{{ app.request.getSchemeAndHttpHost() }}/assets/css/style.css" rel="stylesheet">
        <link href="{{ app.request.getSchemeAndHttpHost() }}/assets/css/responsive.css" rel="stylesheet">
        {% block stylesheets %}
        {% endblock %}

        {% block javascripts %}
{% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    {% if app.user %}
        {% include "widgets/_logged.html.twig" %}
    {% else%}
        {% include "widgets/_unlogged.html.twig" %}
    {% endif %}
    <body data-anm=".anm">
        {% block body %}{% endblock %}
    </body>
    <div>
        {% block footer %}
        {% include "widgets/_footer.html.twig" %}
        {% endblock %}
    </div>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/jquery.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/popper.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/chosen.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/bootstrap.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/jquery.fancybox.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/jquery.modal.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/mmenu.polyfills.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/mmenu.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/appear.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/anm.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/ScrollMagic.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/rellax.min.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/owl.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/wow.js"></script>
    <script src="{{ app.request.getSchemeAndHttpHost() }}/assets/js/script.js"></script>
</html>
